                <BorderPane layoutX="25.0" layoutY="24.0" prefHeight="25.0" prefWidth="240.0">
                    <left>
                        <Label prefHeight="23.0" prefWidth="118.0" styleClass="main-text-style" text="Event ID:" BorderPane.alignment="CENTER" />
                    </left>
                    <center>
                        <TextField fx:id="eventIdField" minWidth="160.0" prefHeight="29.0" prefWidth="160.0" styleClass="main-input-style" BorderPane.alignment="CENTER" />
                    </center>
                </BorderPane>
                <BorderPane layoutX="25.0" layoutY="64.0" prefHeight="25.0" prefWidth="240.0">
