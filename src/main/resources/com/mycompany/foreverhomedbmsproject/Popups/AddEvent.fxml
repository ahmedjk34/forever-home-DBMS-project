                        <Label prefHeight="23.0" prefWidth="118.0" styleClass="main-text-style" text="Event Name:" BorderPane.alignment="CENTER" />
                    </left>
                    <center>
                        <TextField fx:id="eventNameField" minWidth="160.0" prefHeight="29.0" prefWidth="160.0" styleClass="main-input-style" BorderPane.alignment="CENTER" />
                    </center>
                </BorderPane>
                <BorderPane layoutX="338.0" layoutY="24.0" prefHeight="25.0" prefWidth="240.0">
                    <left>
                        <Label prefHeight="23.0" prefWidth="118.0" styleClass="main-text-style" text="Event Time:" BorderPane.alignment="CENTER" />
                    </left>
                    <center>
                        <TextField fx:id="eventTimeField" minWidth="160.0" prefHeight="29.0" prefWidth="160.0" styleClass="main-input-style" BorderPane.alignment="CENTER" />
                    </center>
                </BorderPane>
                <BorderPane layoutX="338.0" layoutY="64.0" prefHeight="25.0" prefWidth="240.0">
                    <left>
                        <Label prefHeight="23.0" prefWidth="118.0" styleClass="main-text-style" text="Event Date:" BorderPane.alignment="CENTER" />
                    </left>
                    <center>
                        <TextField fx:id="eventDateField" minWidth="160.0" prefHeight="29.0" prefWidth="160.0" styleClass="main-input-style" BorderPane.alignment="CENTER" />
                    </center>
                </BorderPane>
                <BorderPane layoutX="25.0" layoutY="104.0" prefHeight="25.0" prefWidth="240.0">
                    <left>
                        <Label prefHeight="23.0" prefWidth="118.0" styleClass="main-text-style" text="Location:" BorderPane.alignment="CENTER" />
                    </left>
                    <center>
                        <TextField fx:id="locationField" minWidth="160.0" prefHeight="29.0" prefWidth="160.0" styleClass="main-input-style" BorderPane.alignment="CENTER" />
                    </center>
                </BorderPane>
                <BorderPane layoutX="338.0" layoutY="104.0" prefHeight="25.0" prefWidth="240.0">
                    <left>
                        <Label prefHeight="23.0" prefWidth="118.0" styleClass="main-text-style" text="Fund Goal:" BorderPane.alignment="CENTER" />
                    </left>
                    <center>
                        <TextField fx:id="fundGoalField" minWidth="160.0" prefHeight="29.0" prefWidth="160.0" styleClass="main-input-style" BorderPane.alignment="CENTER" />
                    </center>
                </BorderPane>
            </children>
        </AnchorPane>
        <Button fx:id="addButton" layoutX="353.0" layoutY="304.0" mnemonicParsing="false" onAction="#handleAddButtonClick" prefHeight="33.0" prefWidth="84.0" styleClass="main-button-style" stylesheets="@../../../../styles/fxml.css" text="Add">
            <graphic>
                <ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../../../assets/add.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>
